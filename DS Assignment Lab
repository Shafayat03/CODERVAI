/**

3. Write a program to create an array of n elements to read the marks of n students
and then count how many students passed [pass marks â‰¥ 40] in the examination

*/





#include<bits/stdc++.h>
using namespace std;
using ll = long long;
int main(){
    
    int n;
    cin >> n;
    
    vector<int>a(n + 5);
    
    for(int i = 1; i <= n ; i++)cin >> a[i];
    int cnt = 0;
    for(int i = 1; i <= n ; i++){
        
        if(a[i] >= 40){
             cnt++;
        }
    }
    
    cout << cnt << '\n';
}

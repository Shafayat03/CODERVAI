/**
1. Write a program to find the largest number and it's location from a given list of
integers.
*/

#include<bits/stdc++.h>
using namespace std;

int main(){


 



 int n,k;
 cin >> n;

 vector<int>a(n+4);

 for(int i = 1; i <= n; i++){
    cin >> a[i];
 }

  int pos = 0;

  int mx = *a.begin();

  for(int i = 1;i <= n; i++){
      if(a[i] > mx ){
         mx = a[i];
         pos = i;

      }
  }

  cout << pos << ' ' << mx << '\n';



return 0;


}






/**
 2.Write a program to calculate the roots of the quadratic equation ax2 + bx + c = 0
where a, b and c are known.

*/



#include<bits/stdc++.h>
using namespace std;
using ll = long long;

#define shafu() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);

int main(){



   int a,b,c;
   cin >> a >> b >> c;


   int d = (b * b)  - 4 * a * c;

   if(d > 0){
       int  x1 = (-b + sqrt(d))/ (2 * a);

       int x2 = (-b - sqrt(d)) / (2 * a);

       cout << x1 << ' ' << x2 << '\n';
   }

   else if(d == 0){
       cout << -b/(2 * a) << '\n';
   }

   else {
    cout << "NO REAL SOLUTION\n";
   }

   /**
     input : 1, -7 , 10
     output : 5, 2
   */
return 0;

}










/**

3. Write a program to create an array of n elements to read the marks of n students
and then count how many students passed [pass marks â‰¥ 40] in the examination

*/





#include<bits/stdc++.h>
using namespace std;
using ll = long long;
int main(){
    
    int n;
    cin >> n;
    
    vector<int>a(n + 5);
    
    for(int i = 1; i <= n ; i++)cin >> a[i];
    int cnt = 0;
    for(int i = 1; i <= n ; i++){
        
        if(a[i] >= 40){
             cnt++;
        }
    }
    
    cout << cnt << '\n';
}




/**

4. Write a program to create an array of n elements and then insert an element to
the list.

**/


#include<bits/stdc++.h>
using namespace std;

int main(){

 int n;
 cin >> n;

 vector<int>a(n+4);

 for(int i = 1; i <= n; i++){
    cin >> a[i];
 }

 int x ;
 cin >> x;

 a[n + 1] = x;


 for(int i = 1; i<= n + 1; i++ )cout << a[i] << ' ';
 cout << '\n';

return 0;


}



/**
5. Write a program to create an array of n elements and then delete an element
from the list. 
**/




#include<bits/stdc++.h>
using namespace std;

int main(){

 int n;
 cin >> n;

 vector<int>a(n+4);

 for(int i = 1; i <= n; i++){
    cin >> a[i];
 }


 int x ;
 cin >> x;

bool check = false;


 for(int i = 1; i<= n; i++ ){
        if(a[i] == x){
            a[i] = a[i+1];
            x = a[i+1];
            check = true;
        }

 }

 if(check) n--;

 for(int i = 1; i<= n ; i++)cout << a[i] << ' ';
 cout << '\n';

return 0;
}



/**
6. Write a program to sort n numbers using Bubble Sort algorithm.

*/


#include<bits/stdc++.h>
using namespace std;

int main(){



 int n;
 cin >> n;

 vector<int>a(n);

 for(int i = 0; i < n; i++){
    cin >> a[i];
 }


 for(int i = 0; i < n; i++){
    for(int j = 0; j < n - i - 1; j++){
        if(a[j] > a[j+1]){
            swap(a[j], a[j+1]);
        }
    }
 }

for(int i = 0; i < n; i++)cout << a[i] << ' ';
cout << '\n';


return 0;
}





/**
7. Write a program to search an element from a list of n numbers using Linear
Search algorithm.

*/

#include<bits/stdc++.h>
using namespace std;

int main(){



 int n;
 cin >> n;

 vector<int>a(n);

 for(int i = 0; i < n; i++){
    cin >> a[i];
 }


 int value;
 cin >> value;
 int fans = -1;



 for(int i = 0; i < n;i++){

      if(a[i] ==  value){

           fans = i;
      }
 }


if(fans != -1) cout << "element found at : " << fans + 1 << '\n';
else cout << "element doesn's exist" << '\n';


return 0;
}













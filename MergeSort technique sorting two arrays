#include<bits/stdc++.h>
using namespace std;


vector<int > merge(vector<int>a, vector<int>b)
{
    vector<int>result;

    int idx1 = 0, idx2 = 0;

    while(idx1 <a.size() and idx2 < b.size())
    {
        if(a[idx1] < b[idx2])
        {
            result.push_back(a[idx1]);
        idx1++;
        }
        else{
            result.push_back(b[idx2]);
           idx2++;
        }
    }


    while(idx1 < a.size())
    {
        result.push_back(a[idx1]);
        idx1++;
    }

    while(idx2 < b.size())
    {
        result.push_back(b[idx2]);
        idx2++;
    }

    return result;
}



int  main()
{


    vector<int> a = {4,5,8,9, 13};
    vector<int> b = {6,7,12, 15, 19, 20};

 vector<int> result = merge(a,b);

 for(int x : result)
 {
     cout << x << " ";
 }
 cout << endl;

}
